<Project>
  <PropertyGroup>
    <Version>1.0.0</Version>
    <Authors>17mkh.com</Authors>
    <NoWarn>$(NoWarn);1591</NoWarn>
    <LangVersion>Latest</LangVersion>
    <TargetFramework>net6.0</TargetFramework>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <RootNamespacePrefix>Mkh</RootNamespacePrefix>
    <AssemblyName>$(RootNamespacePrefix).$(MSBuildProjectName)</AssemblyName>
    <RootNamespace>$(AssemblyName)</RootNamespace>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RunAnalyzersDuringLiveAnalysis>false</RunAnalyzersDuringLiveAnalysis>
    <PackageOutputPath>$(SolutionDir)\_packages</PackageOutputPath>
    <Company></Company>
    <Copyright>Copyright：2020-2099</Copyright>
    <PackageProjectUrl>https://www.17mkh.com/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/17mkh/Mkh</RepositoryUrl>
  </PropertyGroup>
</Project>
